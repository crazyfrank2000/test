     "input": [
      "def calcUnit(portfolio_value,ATR):\n",
      "    '''\n",
      "     \u8ba1\u7b97unit,\u6ce8\u610f\u80a1\u6570\u4e3a100\u7684\u6574\u6570\u500d\n",
      "    '''\n",
      "    value = portfolio_value * trade_percent         \n",
      "    return int((value/ATR)/100)*100                             "
     ],
